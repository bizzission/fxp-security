{
    "name": "fxp/security",
    "type": "library",
    "description": "A Extended Role-Based Access Control with sharing rules",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/fxpio/fxp-security",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "doctrine/collections": "^1.5.0",
        "doctrine/event-manager": "^1.0.0",
        "doctrine/persistence": "^1.0.0",
        "fxp/doctrine-extra": "^2.0.0",
        "psr/cache": "^1.0.0",
        "symfony/event-dispatcher": "^4.3.0",
        "symfony/expression-language": "^4.3.0",
        "symfony/security": "^4.3.0",
        "symfony/validator": "^4.3.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.6.0",
        "doctrine/orm": "^2.6.0",
        "fxp/cache": "^2.0.0",
        "fxp/doctrine-extensions": "^2.0.0",
        "symfony/config": "^4.3.0",
        "symfony/dependency-injection": "^4.3.0",
        "symfony/finder": "^4.3.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\Security\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
