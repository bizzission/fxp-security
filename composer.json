{
    "name": "fxp/security",
    "type": "library",
    "description": "A Extended Role-Based Access Control with sharing rules",
    "keywords": ["RBAC", "E-RBAC", "Permission", "Sharing", "Security"],
    "homepage": "https://github.com/fxpio/fxp-security",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "doctrine/collections": "^1.6",
        "doctrine/event-manager": "^1.1",
        "doctrine/persistence": "^1.3",
        "fxp/config": "dev-master as 2.0",
        "fxp/doctrine-extra": "dev-master as 2.0",
        "psr/cache": "^1.0",
        "symfony/event-dispatcher": "^5.0.0",
        "symfony/expression-language": "^5.0.0",
        "symfony/http-kernel": "^5.0.0",
        "symfony/property-access": "^5.0.0",
        "symfony/security-core": "^5.0.0",
        "symfony/validator": "^5.0.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.10",
        "doctrine/orm": "^2.7",
        "fxp/cache": "dev-master as 2.0",
        "fxp/doctrine-extensions": "dev-master as 2.0",
        "symfony/config": "^5.0.0",
        "symfony/dependency-injection": "^5.0.0",
        "symfony/finder": "^5.0.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\Security\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-cache.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-doctrine-extra.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bizzission/fxp-config.git"
        }
    ]
}
